include aux

func gohome() {
    if(home is ahead) then {
        move(1, gohome);
        drop();
        call search;
    } else {
        if(randint(3) is 0) then {
            turn(5);
        } else {
            if(randint(2) is 0) then {
                turn(1);
            } else {
                move(1, gohome);
            }
        }
    }
}