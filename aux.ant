func search() {
    if(food is ahead) then {
        move(1, search);
        pickup(search);
    } else {
        call flip;
    }

    call gohome;
}

func flip() {
    if(randint(3) is 0) then {
        turn(5);
    } else {
        if(randint(2) is 0) then {
            turn(1);
        } else {
            move(1, flip);
        }
    }

    call search;
}