func SearchFood() {

    mark(1);

    if (
        is(food, left)
    )
    then {
        turn(5);
    }
    else {
        do {
            turn(1);
        }
        if (
            is(food,right)
        )
    }


    if (
        is(friend,ahead)
    )
    then {
        turn(1);
        move(1);
        turn(5);
        move(1);
        turn(1);
    }
    else {
        move(1);
    }

    pickup(SearchFood);
    turn(3);
    call BackFood;

    if (
        eq(randint(3),0)
    )
    then {
        do {
            turn(1);
        }
        if (
            eq(randint(2),0)
        )
    }
    else {
        turn(5);
    }


func BackFood {
    do {
        drop();
        turn(3);
        call SearchFood;
    }
    if (
        is(home,here)
    )
    
    if (
        is(bit(1),left)
    )
    then {
        turn(5);
    }
    else {
        do {
            turn(1);
        }
        if (
            is(bit(1),right)
        )
    }

    move(1);
}