SearchFood:

    mark(0)

    test(food) in {
        case ahead : {
            move(1)
            pickup(SearchFood)
            turn(3)
            goto BackFood
        }
        case left : {
            turn(5)
            move(1)
            pickup(SearchFood)
            turn(3)
            goto BackFood
        }
        case right : {
            turn(1)
            move(1)
            pickup(SearchFood)
            turn(3)
            goto BackFood
        }
    }

    test(randint(3)) in {
        case 1 : {
            turn(5)
        }
        case 2 : {
            turn(1)
        }
        move(1)
    }

    goto SearchFood


BackFood:
do {
    drop()
    turn(3)
    goto SearchFood
}
if (
    is(home,here)
)
test (bit(1)) in {
    case right : {
        turn(1)
    }
    case left : {
        turn(5)
    }
}
move(1)
goto BackFood